{
  "name": "vote-copro-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "typecheck": "tsc --noEmit",
    "export:schema": "ts-node -r tsconfig-paths/register scripts/exportSchema.ts",
    "codegen": "npm run export:schema && dotenv -e .env -- npx graphql-codegen",
    "codegen:local": "dotenv -e .env.local -- npx graphql-codegen",
    "codegen:prod": "dotenv -e .env.production -- npx graphql-codegen",
    "codegen:watch": "dotenv -e .env.local -- npx graphql-codegen --watch"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^5.10.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@upstash/ratelimit": "^0.4.2",
    "@upstash/redis": "^1.22.0",
    "framer-motion": "^12.23.12",
    "graphql": "^16.7.1",
    "graphql-yoga": "^5.15.2",
    "lucide-react": "^0.543.0",
    "next": "latest",
    "nodemailer": "^7.0.6",
    "react": "latest",
    "react-dom": "latest",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^9.0.0",
    "zod": "^3.23.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typed-document-node": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.19.13",
    "@types/react": "^18.2.22",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prisma": "^5.10.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
